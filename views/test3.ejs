                            <% for (var i = DynamicData.length-10; i < DynamicData.length; i++) {
                            %>
                            <tr>
                                <td><%= DynamicData[i].LTID %></td>
                                <td><%= DynamicData[i].pulse %></td>
                                <td><%= DynamicData[i].lat %></td>
                                <td><%= DynamicData[i].lon %></td>
                                <td><%= DynamicData[i].time.toLocaleString() %></td>
                            </tr>
                            <%
                            } %>

                        </table>
                    </div>
                </div>
            </div>

        </div>
