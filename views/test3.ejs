<br><br><br><br><br>
<hr>


<script>
    var a=[ ];
    var b=[ ];
</script>

<% for (var i = DynamicData.length-10; i < DynamicData.length; i++) { %>
<script>
    a.push(<%= DynamicData[i].pulse %>);
    b.push('<%= DynamicData[i].time %>');
</script>
<% } %>



<script>
    var j=0;

    var chart = Highcharts.chart('container', {

        title: {
            text: '최근 맥박 변화'
        },
        xAxis: {
            categories: b,
            id: 'x-axis'
        },
        yAxis: {
            min : 30,
            max: 120,
            title: {
                text: 'pulse'
            }
        },

        subtitle: {
            floating: true,
            align: 'left',
            x: 100,
            y: 70

        },

        series: [{
            name:"Pulse",
            data : a

        }]

    });
</script>
